{
    "appenders": {
        "outLog": {
            "type": "stdout",
            "category": "outLog",
            "layout": {
                "type": "pattern",
                "pattern": "%d{yyyy-MM-dd hh:mm:ss} %p: %m%n"
            }
        }
    },
    "categories": {
        "default": {
            "level": "info",
            "appenders": [ "outLog" ]

        }
    }
}